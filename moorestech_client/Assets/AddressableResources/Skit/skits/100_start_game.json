      "Rotation": [
        2.510181,
        191.2412,
        0.02663064
      ],
      "id": 109
    },
    {
      "type": "text",
      "backgroundColor": "#ffffff",
      "characterId": "chr_002",
      "body": "何を言っているのですか？\nあなたは追放されたのです。",
      "id": 20,
      "overideCharacterName": "？？？"
    },
    {
      "type": "text",
      "backgroundColor": "#ffffff",
      "characterId": "chr_002",
      "body": "国王陛下……つまり、あなたのお父様の命令により、\n姫さまは惑星セレスタルから永久追放処分となりました。",
      "id": 21,
      "overideCharacterName": "？？？"
    },
    {
      "type": "emote",
      "backgroundColor": "#ffffff",
      "character": "chr_001",
      "emotion": "Emote_Blink",
      "duration": 0.2,
      "weight": "0",
      "id": 116
    },
    {
      "type": "emote",
      "backgroundColor": "#ffffff",
