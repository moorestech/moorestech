{
  "mcpServers": {
    "moorestech_server": {
      "type": "http",
      "url": "http://localhost:56901/mcp"
    },
    "moorestech_client": {
      "type": "http",
      "url": "http://localhost:56902/mcp"
    },
    "serena": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena-mcp-server",
        "--context",
        "ide-assistant",
        "--project",
        "/Users/sato-katsumi/moorestech",
        "--enable-web-dashboard", 
        "False"
      ],
      "env": {}
    },
    "kiri": {
      "command": "npx",
      "args": ["kiri-mcp-server@latest", "--repo", ".", "--db", ".kiri/index.duckdb", "--watch"]
    }
  }
}
